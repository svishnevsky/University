@using GrSU.University.Clients.Resources
@using GrSU.University.Clients.Resources.Entities
@model GrSU.University.Clients.Web.Models.Students.StudentModel

@{
    ViewBag.Title = Student.TitleEdit;
}

<h2>@Student.TitleEdit</h2>

@using (Html.BeginForm("Index", "Student"))
{

    @Html.HttpMethodOverride("PUT")
    @Html.AntiForgeryToken()

    @Html.HiddenFor(m => m.Id)

    @Html.EditorForModel()
}

<div>
    @Html.ActionLink(Navigation.GoToList, "Index", "Student")
</div>
